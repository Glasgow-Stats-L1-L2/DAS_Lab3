lifeExp.continent <- gapminder2007 %>%
group_by(continent) %>%
summarize(median = median(lifeExp), mean = mean(lifeExp))
lifeExp.by.continent <- gapminder2007 %>%
group_by(continent) %>%
summarize(median = median(lifeExp), mean = mean(lifeExp))
mean.africa <- lifeExp.by.continent %>%
filter(continent == "Africa") %>%
pull(mean)
